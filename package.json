{
  "name": "ng-log-service",
  "version": "0.0.5",
  "description": "Logging Service for Angular 5.x.x",
  "repository": {
    "type": "git",
    "url": "https://github.com/unisoftapps/ng-log-service.git"
  },
  "keywords": [
    "angular 5",
    "angular5",
    "angular",
    "log",
    "log service",
    "logging",
    "log module",
    "log4net"
  ],
  "main": "dist/index.js",
  "author": "unisoftapps",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/unisoftapps/ng-log-service/issues"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/unisoftapps/ng-log-service",
  "scripts": {
    "clean:pre": "rimraf dist",
    "clean:post": "rimraf \"src/**/*.ngfactory.ts\" \"**/*.ngsummary.json\" \"dist/node_modules/\"",
    "copy": "cpy package.json README.md dist",
    "build:js": "ngc -p tsconfig.dist.json",
    "build:umd": "rollup -c rollup.config.js",
    "prebuild": "npm run test && npm run clean:pre",
    "postbuild": "npm run clean:post && npm run copy",
    "build": "npm run build:js && npm run build:umd"
  },
  "dependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/common": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/router": "^5.0.0",
    "rxjs": "^5.5.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/compiler": "^5.0.0",
    "@angular/compiler-cli": "^5.0.0",
    "typescript": "~2.6.2",
    "cpy-cli": "^1.0.1",
    "rimraf": "^2.5.4",
    "rollup": "^0.41.1"
  }
}